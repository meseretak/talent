{
  "python.formatting.provider": "yapf",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "prettier.tabWidth": 2,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#17337a",
    "activityBar.background": "#17337a",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#d7285a",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#17337a",
    "statusBar.background": "#0f214f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#17337a",
    "statusBarItem.remoteBackground": "#0f214f",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#0f214f",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#0f214f99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#0f214f"
}
